<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="755.0" prefWidth="1010.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clarify.View.HomeController">
   <children>
      <PieChart fx:id="lifePieChart" layoutX="62.0" layoutY="69.0" prefHeight="275.0" prefWidth="405.0" title="Pie Chart of My Life" AnchorPane.leftAnchor="62.0" AnchorPane.topAnchor="69.0" />
      <BarChart fx:id="dailyBarChart" layoutX="75.0" layoutY="369.0" prefHeight="275.0" prefWidth="405.0" title="Daily Breakdown" AnchorPane.leftAnchor="75.0" AnchorPane.topAnchor="369.0">
        <xAxis>
          <CategoryAxis label="Top 5 Activities" side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" label="Hours per day" side="LEFT" />
        </yAxis>
      </BarChart>
      <BarChart fx:id="weeklyBarChart" layoutX="506.0" layoutY="364.0" prefHeight="275.0" prefWidth="405.0" title="Weekly Breakdown">
         <xAxis>
            <CategoryAxis label="Top 5 Activities" side="BOTTOM" fx:id="xWeekly" />
         </xAxis>
         <yAxis>
            <NumberAxis fx:id="yWeekly" label="Hours per day" side="LEFT" />
         </yAxis>
      </BarChart>
      <Button fx:id="logTime" layoutX="603.0" layoutY="135.0" mnemonicParsing="false" onAction="#addLogPressed" style="-fx-background-color: cornflowerblue;" text="+ Log Time" textFill="WHITE">
         <font>
            <Font name="Avenir Black" size="15.0" />
         </font>
      </Button>
      <Label layoutX="40.0" layoutY="14.0" prefHeight="33.0" prefWidth="407.0" text="Your Dashboard" textFill="CORNFLOWERBLUE">
         <font>
            <Font name="Avenir Black" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
