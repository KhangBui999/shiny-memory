<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="755.0" prefWidth="1010.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clarify.View.HomeController">
   <children>
      <PieChart fx:id="lifePieChart" layoutX="64.0" layoutY="27.0" prefHeight="303.0" prefWidth="402.0" title="Pie Chart of My Life" />
      <BarChart fx:id="dailyBarChart" layoutX="511.0" layoutY="27.0" prefHeight="303.0" prefWidth="402.0" title="Daily Breakdown">
        <xAxis>
          <CategoryAxis label="Top 5 Activities" side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" label="Hours per day" side="LEFT" />
        </yAxis>
      </BarChart>
      <LineChart fx:id="trendsLineChart" layoutX="117.0" layoutY="372.0" prefHeight="304.0" prefWidth="404.0" title="Weekly Trends">
        <xAxis>
          <CategoryAxis label="Weeks" side="BOTTOM" fx:id="xTrends" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yTrends" label="\% Hours per week on activity " side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="904.0" layoutY="282.0" mnemonicParsing="false" onAction="#handleAddEntryBtn" text="Add Entry" />
      <BarChart fx:id="weeklyBarChart" layoutX="534.0" layoutY="372.0" prefHeight="304.0" prefWidth="355.0" title="Weekly Breakdown">
        <xAxis>
          <CategoryAxis label="Top 5 activities" side="BOTTOM" fx:id="xWeekly" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yWeekly" label="Hours per week" side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
